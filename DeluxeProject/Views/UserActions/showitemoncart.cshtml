@model IEnumerable<DeluxeModel.product>
@{
    ViewBag.Title = "showitemoncart";
    Layout = "~/Views/Shared/_UserMaster.cshtml";
}

@foreach(var item in Model)
{
    <table>
        <tbody>
            <tr>
                <td>
                    @item.prd_name
                </td>
            </tr>
        </tbody>
    </table>
}

